<?xml version="1.0" ?>
<sdf version="1.8">
   <world name="world2">
      <physics name="1ms" type="ignored">
         <max_step_size>0.0001</max_step_size>
         <real_time_factor> 1</real_time_factor>
      </physics>
      <plugin filename="libignition-gazebo-physics-system.so" name="ignition::gazebo::systems::Physics">
